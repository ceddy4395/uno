<Project>
	<!--
		Include all required PackageReference to be available before any target is executed
		in order for VS and C# Dev Kit to show nuget references in their respective solution explorers.
		The version is not required, and VS/Code waits for some design-time targets to be executed to evaluate it.
	-->
	<Import Project="$(MSBuildThisFileDirectory)Uno.Implicit.Packages.ProjectSystem.Uno.targets" />

	<ItemGroup Condition="$(_IsExecutable) == 'true'">
		<PackageReference Include="Uno.WinUI.WebAssembly" ProjectSystem="true" />
		<PackageReference Include="Uno.WinUI.MediaPlayer.WebAssembly" ProjectSystem="true" Condition="$(UnoFeatures.Contains(';MediaElement;'))"/>
		<PackageReference Include="Microsoft.Windows.Compatibility" ProjectSystem="true" />
		<PackageReference Include="Uno.Extensions.Logging.WebAssembly.Console" ProjectSystem="true" />
		<PackageReference Include="Uno.Wasm.Bootstrap" ProjectSystem="true" />
		<PackageReference Include="Uno.Wasm.Bootstrap.DevServer" ProjectSystem="true" />
	</ItemGroup>
</Project>
